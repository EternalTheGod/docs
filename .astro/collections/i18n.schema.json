          "type": "string"
        },
        "links": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "sitemap": {
          "type": "object",
          "properties": {
            "include": {
              "type": "boolean"
            },
            "pageTitle": {
              "type": "string"
            },
            "linkInclusionRules": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": []
            }
          },
          "additionalProperties": false,
          "default": {
            "linkInclusionRules": [
              "**/*"
            ]
          }
        },
        "graph": {
          "type": "object",
          "properties": {
            "actions": {
              "type": "array",
              "items": {
                "type": "string",
                "enum": [
                  "fullscreen",
                  "depth",
                  "reset-zoom",
                  "render-arrows",
                  "render-external",
                  "render-unresolved",
                  "settings"
                ]
              },
              "default": [
                "fullscreen",
                "depth",
                "reset-zoom",
                "render-arrows",
                "settings"
              ]
            },
            "tagStyles": {
              "type": "object",
              "additionalProperties": {
                "type": "object",
                "properties": {
                  "shape": {
                    "type": "string",
                    "enum": [
                      "circle",
                      "square",
                      "triangle",
                      "polygon",
                      "star"
                    ],
                    "default": "circle"
                  },
                  "shapeSize": {
                    "type": "number",
                    "exclusiveMinimum": 0,
                    "errorMessage": {
                      "exclusiveMinimum": "Shape size may not be zero or negative"
                    },
                    "default": 10
                  },
                  "shapeColor": {
                    "anyOf": [
                      {
                        "type": "string",
                        "enum": [
                          "backgroundColor",
                          "nodeColor",
                          "nodeColorVisited",
                          "nodeColorCurrent",
                          "nodeColorUnresolved",
                          "nodeColorExternal",
                          "nodeColorTag",
                          "nodeColor1",
                          "nodeColor2",
                          "nodeColor3",
                          "nodeColor4",
                          "nodeColor5",
                          "nodeColor6",
                          "nodeColor7",
                          "nodeColor8",
                          "nodeColor9",
                          "linkColor"
                        ]
                      },
                      {
                        "type": "string",
                        "const": "stroke"
                      }
                    ],
                    "default": "nodeColor"
                  },
                  "shapePoints": {
                    "type": "number",
                    "minimum": 2,
                    "errorMessage": {
                      "minimum": "The number of points for the shape may not be smaller than 2"
                    }
                  },
                  "shapeRotation": {
                    "anyOf": [
                      {
                        "type": "number"
                      },
                      {
                        "type": "string",
                        "const": "random"
                      }
                    ]
                  },
                  "shapeCornerRadius": {
                    "anyOf": [
                      {
                        "type": "number",
                        "minimum": 0,
                        "errorMessage": {
                          "minimum": "Shape corner radius may not be negative"
                        }
                      },
                      {
                        "type": "string"
                      }
                    ]
                  },
                  "cornerType": {
                    "type": "string",
                    "enum": [
                      "normal",
                      "round",
                      "bevel"
                    ]
                  },
                  "strokeWidth": {
                    "type": "number",
                    "minimum": 0,
                    "default": 0
                  },
                  "strokeColor": {
                    "anyOf": [
                      {
                        "$ref": "#/definitions/i18n/properties/graph/properties/tagStyles/additionalProperties/properties/shapeColor/anyOf/0"
                      },
                      {
                        "type": "string",
                        "const": "inherit"
                      }
                    ]
                  },
                  "strokeCornerRadius": {
                    "$ref": "#/definitions/i18n/properties/graph/properties/tagStyles/additionalProperties/properties/shapeCornerRadius"
                  },
                  "colliderScale": {
                    "type": "number",
                    "minimum": 0,
                    "default": 1
                  },
                  "nodeScale": {
                    "type": "number",
                    "minimum": 0,
                    "default": 1
                  },
                  "neighborScale": {
                    "type": "number",
                    "minimum": 0,
                    "default": 0.5
                  }
                },
                "additionalProperties": false
              },
              "default": {}
            },
            "tagRenderMode": {
              "type": "string",
              "enum": [
                "none",
                "node",
                "same",
                "both"
              ],
              "default": "none"
            },
            "enableDrag": {
              "type": "boolean",
              "default": true
            },
            "enableZoom": {
              "type": "boolean",
              "default": true
            },
            "enablePan": {
              "type": "boolean",
              "default": true
            },
            "enableHover": {
              "type": "boolean",
              "default": true
            },
            "enableClick": {
              "type": "string",
              "enum": [
                "auto",
                "disable",
                "click",
                "dblclick"
              ],
              "default": "auto"
            },
            "depth": {
              "type": "number",
              "default": 1
            },
            "depthDirection": {
              "type": "string",
              "enum": [
                "both",
                "incoming",
                "outgoing"
              ],
              "default": "both"
            },
            "followLink": {
              "type": "string",
              "enum": [
                "same",
                "new-tab",
                "graph"
              ],
              "default": "same"
            },
            "scale": {
              "type": "number",
              "exclusiveMinimum": 0,
              "errorMessage": {
                "exclusiveMinimum": "Graph scale may not be zero or negative"
              },
              "default": 1.1
            },
            "minZoom": {
              "type": "number",
              "exclusiveMinimum": 0,
              "errorMessage": {
                "exclusiveMinimum": "Graph zoom may not be zero or negative"
              },
              "default": 0.05
            },
            "maxZoom": {
              "type": "number",
              "exclusiveMinimum": 0,
              "errorMessage": {
                "exclusiveMinimum": "Graph zoom may not be zero or negative"
              },
              "default": 4
            },
            "renderLabels": {
              "type": "boolean",
              "default": true
            },
            "renderArrows": {
              "type": "boolean",
              "default": false
            },
            "renderUnresolved": {
              "type": "boolean",
              "default": false
            },
            "renderExternal": {
              "type": "boolean",
              "default": true
            },
            "scaleLinks": {
              "type": "boolean",
              "default": true
            },
            "scaleArrows": {
              "type": "boolean",
              "default": true
            },
            "minZoomArrows": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Minimum zoom for arrow rendering may not be negative"
              },
              "default": 0.8
            },
            "labelOpacityScale": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Opacity scale for labels may not be negative"
              },
              "default": 1.3
            },
            "labelMutedOpacity": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Opacity scale for muted labels may not be negative"
              },
              "default": 0
            },
            "labelHoverOpacity": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Opacity scale for hovered labels may not be negative"
              },
              "default": 1
            },
            "labelAdjacentOpacity": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Opacity scale for hovered labels may not be negative"
              },
              "default": 1
            },
            "labelFontSize": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Label font size may not be negative"
              },
              "default": 12
            },
            "labelHoverScale": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Label hover scale may not be negative"
              },
              "default": 1
            },
            "labelOffset": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Label offset may not be negative"
              },
              "default": 10
            },
            "labelHoverOffset": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Label hover offset may not be negative"
              },
              "default": 14
            },
            "zoomDuration": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Zoom duration may not be negative"
              },
              "default": 75
            },
            "zoomEase": {
              "type": "string",
              "enum": [
                "in_quad",
                "out_quad",
                "in_out_quad",
                "linear"
              ],
              "default": "out_quad"
            },
            "hoverDuration": {
              "type": "number",
              "minimum": 0,
              "errorMessage": {
                "minimum": "Hover duration may not be negative"
              },
              "default": 200
            },
            "hoverEase": {
              "$ref": "#/definitions/i18n/properties/graph/properties/zoomEase",
              "default": "out_quad"
            },
